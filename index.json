{
  "$schema": "http://json.schemastore.org/prettierrc",
  "singleQuote": true,
  "trailingComma": "none",
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "quoteProps": "preserve"
      }
    },
    {
      "files": "*.mdx",
      "options": {
        "embeddedLanguageFormatting": "off"
      }
    }
  ]
}
